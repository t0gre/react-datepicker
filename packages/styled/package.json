  },
  "devDependencies": {
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.9",
    "csstype": "^3.0.0",
    "date-fns": "^2.14.0",
    "styled-components": "^5.1.1"
  },
  "dependencies": {
    "@datepicker-react/hooks": "^2.7.0",
    "styled-system": "^5.1.5"
